{
  "name": "project_catalogue_backend",
  "version": "1.0.0",
  "description": "The backend application for project Catalogue.",
  "private": true,
  "main": "./dist/server.js",
  "scripts": {
    "build": "npm run tsc",
    "tsc": "tsc",
    "lint": "tslint -c ./tslint.json -p ./tsconfig.json",
    "watch": "npm run tsc -- -w",
    "prestart": "npm run build",
    "start": "node ./dist/server.js",
    "start-watch": "nodemon -L --inspect=0.0.0.0:9229 ./dist/server.js",
    "migrations-dev": "db-migrate -e development",
    "migrations-prod": "db-migrate -e production"
  },
  "author": "Pedro Henriques <pedro@pedrojhenriques.com> (http://pedrojhenriques.com)",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.3",
    "db-migrate": "^0.11.3",
    "db-migrate-mysql": "^1.1.10",
    "express": "^4.16.4",
    "node-fetch": "^2.2.0",
    "redis": "^2.8.0",
    "uuid": "^3.3.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.0",
    "@types/node-fetch": "^2.1.2",
    "@types/redis": "^2.8.7",
    "@types/uuid": "^3.4.4",
    "nodemon": "^1.18.3",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.1.3"
  }
}
